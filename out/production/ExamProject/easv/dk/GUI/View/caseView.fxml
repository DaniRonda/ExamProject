<AnchorPane prefHeight="650.0" prefWidth="900.0" style="-fx-background-color: #D6E7FB;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="easv.dk.GUI.Controller.CaseViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="71.0" prefWidth="902.0" style="-fx-background-color: #EFEFE6;" />
      <TextArea fx:id="textAreaCase" layoutX="169.0" layoutY="189.0" prefHeight="303.0" prefWidth="685.0" />
      <Button fx:id="btnSaveCase" layoutX="725.0" layoutY="513.0" mnemonicParsing="false" onAction="#saveCase" prefHeight="25.0" prefWidth="57.0" text="Save" />
      <Label layoutX="29.0" layoutY="124.0" text="Case name">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField layoutX="169.0" layoutY="124.0" prefHeight="25.0" prefWidth="189.0" />
      <Button fx:id="btnCancelCreatingCase" layoutX="804.0" layoutY="513.0" mnemonicParsing="false" onAction="#cancelCreatingCase" text="Cancel" />
      <Label layoutX="29.0" layoutY="176.0" text="Case description">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnReturn" layoutX="801.0" layoutY="25.0" mnemonicParsing="false" onAction="#returnToPreviousView" prefHeight="25.0" prefWidth="57.0" text="Return" />
