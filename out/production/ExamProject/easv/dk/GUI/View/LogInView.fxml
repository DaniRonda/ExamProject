<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="387.0" prefWidth="481.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="easv.dk.GUI.Controller.LogInController">
   <children>
      <TextField fx:id="TextFieldEmail" layoutX="102.0" layoutY="204.0" prefHeight="38.0" prefWidth="276.0" promptText="E-Mail" />
      <Button fx:id="BtnCancel" layoutX="257.0" layoutY="328.0" mnemonicParsing="false" onAction="#cancelLogIn" text="Cancel" />
      <Button fx:id="ButtonSignIn" layoutX="323.0" layoutY="328.0" mnemonicParsing="false" onAction="#signIn" text="Sign In " />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="144.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Pictures/LogInIcon.jpg" />
         </image>
      </ImageView>
      <PasswordField fx:id="TextFieldPassword" layoutX="102.0" layoutY="262.0" prefHeight="38.0" prefWidth="276.0" promptText="Password" />
   </children>
</AnchorPane>
