<AnchorPane prefHeight="650.0" prefWidth="900.0" style="-fx-background-color: #D6E7FB;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="easv.dk.GUI.Controller.TemplateCaseViewController">
   <children>
      <HBox alignment="CENTER" layoutX="-1.0" prefHeight="71.0" prefWidth="902.0" style="-fx-background-color: #EFEFE6;" />
      <TextArea fx:id="textAreaCase" layoutX="169.0" layoutY="189.0" prefHeight="303.0" prefWidth="685.0" />
      <Text layoutX="29.0" layoutY="201.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Case description">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Text>
      <Button fx:id="buttonSave" layoutX="725.0" layoutY="513.0" mnemonicParsing="false" onAction="#saveCase" prefHeight="25.0" prefWidth="57.0" text="Save" />
      <Button fx:id="buttonReturn" layoutX="754.0" layoutY="23.0" mnemonicParsing="false" onAction="#returnTo" prefHeight="25.0" prefWidth="57.0" text="Return" />
      <Button fx:id="buttonLogOut" layoutX="830.0" layoutY="23.0" mnemonicParsing="false" onAction="#logOut" text="Log out" />
      <Label layoutX="29.0" layoutY="124.0" text="Case name">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField layoutX="169.0" layoutY="124.0" prefHeight="25.0" prefWidth="189.0" />
